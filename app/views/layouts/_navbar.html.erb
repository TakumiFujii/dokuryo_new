<header>
  <nav class = "navbar navbar-inverse navbar-dark bf-dark fixed-top">
    <div class = "container">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">Dokuryo</a>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class = "nav navbar-nav navbar-right">
          <% if logged_in? %>
            <li><%= link_to new_book_path do %><span class = "glyphicon glyphicon-book"></span> 本の検索<% end %></li>
            <li><a href = "#">レビュー一覧</a></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle text-ellipsis" data-toggle="dropdown">
                <%= image_tag current_user.image_url, class: "topBarIcon" %>
                <span><%= current_user.nickname %></span>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "マイページ", user_path(current_user) %></li>
                <li class="divider show-narrowed"></li>
                <li><%= link_to "プロフィール編集", profile_user_path(current_user)%></li>
                <li class="divider"></li>
                <li><%= link_to 'ログアウト', logout_path, method: :delete %></li>
              </ul>
            </li>
          <% else %>
            <li><%= link_to "始める", signup_path %></li>
            <li><%= link_to 'ログイン', login_path %></li>
          <% end %>
        </ul>
      </div>
      
      </ul>
      <ul class="nav pull-right topbar-right-nav">
        
      </ul>
    
    </div>
    
  </nav>
